﻿{
    "TimberWinR": {
        "Inputs": {
            "WindowsEvents": [
              {
                "source": "Application",
                "binaryFormat": "PRINT",
                "resolveSIDS": true
              }
            ]
        },
        "Filters": [
            {
                "grok": {
                    "condition": "\"[EventTypeName]\" == \"Information Event\"",
                    "match": [
                        "Message",
                        ""
                    ]
                }
            }
        ],
        "Outputs": {
            "Stdout": [
                {
                    "_comment": "Output to Console"
                }
            ]
        }
    }
}
